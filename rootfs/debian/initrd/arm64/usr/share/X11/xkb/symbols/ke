




default partial alphanumeric_keys
xkb_symbols "swa" {

 include "us"

 name[Group1]= "Swahili (Kenya)";

 key <AC10> { [ semicolon, colon, 0x01000324, 0x01000324 ] }; 
 key <AC11> { [apostrophe, quotedbl, 0x01000301, 0x01000308 ] }; 
 key <TLDE> { [ grave, asciitilde, 0x01000300, 0x01000303 ] }; 
 key <AE06> { [ 6, asciicircum, 0x01000302 ] }; 
 key <AE08> { [ 8, asterisk, 0x01000307, 0x01000323 ] }; 
 key <AE09> { [ 9, parenleft, 0x01000306 ] }; 
 key <AE10> { [ 0, parenright, 0x0100030c ] }; 
 key <AE11> { [ minus, underscore, 0x01000304, 0x01000331 ] }; 

 include "level3(ralt_switch)"
};

























partial alphanumeric_keys
xkb_symbols "kik" {

 include "latin(type4)"

 name[Group1]= "Kikuyu";

 key <TLDE> { [ grave, asciitilde ] };
 key <AE01> { [ 1, exclam ] };
 key <AE02> { [ 2, at ] };
 key <AE03> { [ 3, numbersign ] };
 key <AE04> { [ 4, dollar ] };
 key <AE05> { [ 5, percent ] };
 key <AE06> { [ 6, asciicircum ] };
 key <AE07> { [ 7, ampersand ] };
