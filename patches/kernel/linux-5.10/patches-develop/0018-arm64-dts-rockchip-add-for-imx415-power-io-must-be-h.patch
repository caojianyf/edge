From ca40b91213fcd6a95fc119fa5235b10628050b86 Mon Sep 17 00:00:00 2001
From: Shiqin Chen <chensq@rock-chips.com>
Date: Thu, 17 Nov 2022 18:09:45 +0800
Subject: [PATCH 18/31] arm64: dts: rockchip: add for imx415(power io must be
 high)

Signed-off-by: Shiqin Chen <chensq@rock-chips.com>
Change-Id: Ie79aedd53d8a07deaf837b2831c58c5e52d312cc
---
 arch/arm64/boot/dts/rockchip/rk3588-toybrick-imx415.dtsi | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)
 mode change 100755 => 100644 arch/arm64/boot/dts/rockchip/rk3588-toybrick-imx415.dtsi

diff --git a/arch/arm64/boot/dts/rockchip/rk3588-toybrick-imx415.dtsi b/arch/arm64/boot/dts/rockchip/rk3588-toybrick-imx415.dtsi
old mode 100755
new mode 100644
index 22a1d87a917b..dcade5539f82
--- a/arch/arm64/boot/dts/rockchip/rk3588-toybrick-imx415.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3588-toybrick-imx415.dtsi
@@ -56,7 +56,7 @@ imx415: imx415@1a {
 		pinctrl-0 = <&mipim0_camera3_clk>;
 		power-domains = <&power RK3588_PD_VI>;
 		avdd-supply = <&vcc_mipicsi0>;
-		pwdn-gpios = <&gpio1 RK_PB0 GPIO_ACTIVE_LOW>;
+		power-gpios = <&gpio1 RK_PB0 GPIO_ACTIVE_HIGH>;
 		reset-gpios = <&gpio4 RK_PC6 GPIO_ACTIVE_LOW>;
 		rockchip,camera-module-index = <0>;
 		rockchip,camera-module-facing = "back";
@@ -183,7 +183,7 @@ imx415_1: imx415_1@1a {
 		pinctrl-0 = <&mipim0_camera1_clk>;
 		power-domains = <&power RK3588_PD_VI>;
 		avdd-supply = <&vcc_mipidcphy0>;
-		pwdn-gpios = <&gpio1 RK_PB5 GPIO_ACTIVE_LOW>;
+		power-gpios = <&gpio1 RK_PB5 GPIO_ACTIVE_HIGH>;
 		reset-gpios = <&gpio2 RK_PC4 GPIO_ACTIVE_LOW>;
 		rockchip,camera-module-index = <0>;
 		rockchip,camera-module-facing = "front";
